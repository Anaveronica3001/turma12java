package aulas;

public class lista4ex2 {

	
	Pograma
	{
		inclua biblioteca Matematica --> Mat
		funcao inicio()
		{

			int peso;
			double altura;
			int imc;
			
			System.out.print("Digite seu peso atual: ");
			peso = leia.next()

			System.out.print("Digite sua altura: ");
			(altura) = leia.next()

			imc=(peso/Mat.potencia(altura,2))
			
			if (imc<18.5);
			{
			System.out.print (" Vocês está abaixo peso");	
			}
			else if (imc>18.5 e imc <25);
			{
			System.out.print ("Peso normal");
			}
			else if (imc >25 e imc <30);
			{
			System.out.print ("Acima do peso");
			}
			else if (imc > 30);
			{
			System.out.print ("Você está Obeso");

				
			}
		}
	}

