public static void main(String[] args) {
}
	int pont[5]
	int pontMaior =0
	
	for((inteiro x=0; x<5; x++)
	{)
		System.out.print("\nDIGITE UM VALOR: ");
		(pont[x]) = leia.nextInt();	
		
		if(pont[x]>pontMaior) 
		{
			pontMaior = pont[x]
		}
		System.out.print("A MAIOR PONTUAÇAO FOI DE:: ",pontoMaior)
	}
	}
